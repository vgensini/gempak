<?xml version="1.0" encoding="UTF-8"?>
<tns:GempakDataAccess xmlns:tns="http://www.example.org/GempakDataAccess" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/GempakDataAccess GempakDataAccess.xsd ">
    
    <!-- optional -->
    <!-- <dbserver host="edex-cloud.unidata.ucar.edu" /> -->

    <filelabel text="AWIPS DB CONFIGURATION FILE" version="1" />
    <filetype type="MFGD" source="MFGRID" />
    <dbtable name="grid" />
    <rows pyfile="GridInfoRetriever" pymeth="getrow" dbkey="NAM12">
	<row name="GRID" />
    </rows>
    <columns pyfile="GridInfoRetriever" pymeth="getinfo" dbkey="NAM12">
	<column name="GDT1" />
	<column name="GTM1" />
	<column name="GDT2" />
	<column name="GTM2" />
	<column name="GLV1" />
	<column name="GLV2" />
	<column name="GVCD" />
	<column name="GPM1" />
	<column name="GPM2" />
	<column name="GPM3" />
    </columns>
    <headers>
	<header name="NAVB" type="MDREAL" length="256" pyfile="GridNavRetriever" pymeth="getnavb" dbkey="NAM12" />
	<header name="ANLB" type="MDREAL" length="128" pyfile="GridNavRetriever" pymeth="getanlb" dbkey="NAM12" />
    </headers>
    <parts>
	<part name="GRID" type="MDGRID" pyfile="GridDataRetriever" pymethdata="getgriddata" pymethhdr="getheader">
	    <parameters>
		<parameter name="GRID" scale="0" offset="0" bits="0" key="-" />
	    </parameters>
	</part>
    </parts>

</tns:GempakDataAccess>
